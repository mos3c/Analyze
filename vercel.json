{
  "builds": [
    {
      "src": "mos3c/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "staticfiles/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "mos3c/wsgi.py"
    }
  ]
}